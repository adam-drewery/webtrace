@{
    ViewData["Title"] = "Home Page";
}

<div id="wrap" style="height: 100%">
    <table id="jqGrid" style="height: 100%; overflow: hidden;"></table>
</div>

@section Scripts{
    <script type="text/javascript">
        $(document).ready(function () {
            $("#jqGrid").jqGrid({
                url: 'http://trirand.com/blog/phpjqgrid/examples/jsonp/getjsonp.php?callback=?&qwery=longorders',
                mtype: "GET",
                styleUI: 'Bootstrap',
                datatype: "jsonp",
                autowidth: true,
                colModel: [
                    { label: 'OrderID', name: 'OrderID', key: true },
                    { label: 'Customer ID', name: 'CustomerID' },
                    { label: 'Order Date', name: 'OrderDate' },
                    { label: 'Freight', name: 'Freight' },
                    { label: 'Ship Name', name: 'ShipName' }
                ],
                height: '100%',
                rownum:''
            });
        });
    </script>
}
